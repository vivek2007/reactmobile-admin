\\n    handleInputFileChange(e) {\\n        this.setState({\\n            [e.target.name]: e.target.files[0]       \\n        });\\n    }\\n    onSelect(selectedList, selectedItem) {\\n    //   if(selectedList != \\\"\\\"){\\n    //     this.setState({prog_error:false})\\n    // }\\n    //   var arr = selectedList;\\n    //   var array = []\\n    //   arr.map((result,i)=>{\\n    //     array.push(result.name);\\n    //   })\\n      this.setState({\\n        selectedValue: selectedList\\n      });\\n    }\\n   \\n    onRemove(selectedList, removedItem) {\\n      if(selectedList == \\\"\\\"){\\n        this.setState({prog_error:true})\\n    }\\n    }\\n    handledateOfEvent(bday){\\n      if(bday != \\\"\\\"){\\n          this.setState({dob_error:false})\\n      }else{\\n        this.setState({dob_error:true})\\n      }\\n      this.setState({dateofevent:bday})\\n  }\\n  handleStatusChange(checked){\\n    this.setState({status : checked})\\n  }\\n    handleValidation(){\\n        let errors = {};\\n        let formIsValid = true;\\n        console.log(\\\"this.state.profile_image\\\",this.state.profile_image)\\n        if(this.state.profile_image && this.state.profile_image.name && !this.state.profile_image.name.match(/.(jpg|jpeg|png)$/i)){\\n          formIsValid = false;\\n          errors[\\\"profile_image\\\"] = \\\"Please select valid image (Ex: jpg | jpeg | png )\\\";\\n          }\\n        if(this.state.profile_image === ''){\\n          formIsValid = false;\\n          errors[\\\"profile_image\\\"] = \\\"Please choose profile image\\\";\\n        }\\n        if(!this.state.title) {\\n            formIsValid = false;\\n            errors[\\\"title\\\"] = \\\"Please enter title\\\";\\n        }\\n        if(!this.state.description){\\n            formIsValid = false;\\n            errors[\\\"description\\\"] = \\\"Please enter description\\\";\\n        }\\n        \\n          if(!this.state.address1){\\n            formIsValid = false;\\n            errors[\\